<?xml version="1.0" encoding="UTF-8"?>

<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>


            <gradient
                android:startColor="#73C6B5"
                android:centerColor="#73C6B5"
                android:centerY="0.50"
                android:endColor="#73C6B5"
                android:angle="270" />
        </shape>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>

                <gradient
                    android:startColor="#16A085"
                    android:endColor="#16A085"
                    android:angle="90" />
            </shape>
        </clip>
    </item>
</layer-list>